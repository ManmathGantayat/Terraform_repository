node {
    
    stage('gitcheckout') { 
        git branch: 'main', url: 'https://github.com/ManmathGantayat/Terraform_repository.git'
    }
    stage('terraform init') {
        dir('Day-2-statefile'){
        sh 'terraform init'
        }
    }
    stage('terraform plan') {
        dir('Day-2-statefile'){
        sh 'terraform plan'
        }
    }
}
